<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>4.2.6.8</Version>
    <authors>s.bokatuk</authors>
    <owners>s.bokatuk</owners>
    <title>Xamarin bindings to the native VENDOR DDD Mac SDK.</title>
    <projectUrl>GITHUB</projectUrl>
    <description>MAUI .Net Mac Xamarin bindings for VENDOR DDD SDK. Alows to use The VENDOR DDD SDK in your app.</description>
    <summary>.Net Xamarin VENDOR DDD Mac SDK, see SITE</summary>
    <tags>Xamarin VENDOR .Net DDD Mac SDK Xamarin Xamarin.Mac MAUI MAUI.Mac .Net Binding</tags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageId>NMSC.Mac</PackageId>
    <AssemblyName>NMSC.Mac</AssemblyName>
    <SynchReleaseVersion>false</SynchReleaseVersion>
    <ReleaseVersion>4.2.6.1</ReleaseVersion>
    <TargetFramework>net8.0-maccatalyst</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>true</ImplicitUsings>
    <IsBindingProject>true</IsBindingProject>
  </PropertyGroup>
  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="\" />
    <ObjcBindingApiDefinition Include="ApiDefinition.cs" />
    <ObjcBindingCoreSource Include="StructsAndEnums.cs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="NMSC.Mac.targets">
      <PackagePath>build/NMSC.Mac.targets</PackagePath>
    </Content>
  </ItemGroup>
</Project>
